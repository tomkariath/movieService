pipeline {

	agent any
	
	stages{
        stage("Runing unit tests") {
			steps{
				withMaven(maven : 'maven363'){
					sh 'mvn clean install'
				}
			}
		}
            
        stage("Staging") {
			steps{
				withMaven(maven : 'maven363'){
					sh 'mvn spring-boot:run'
				}
			}
        }
    }
}